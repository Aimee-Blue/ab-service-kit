{
  "name": "@aimee-blue/ab-service-kit",
  "version": "0.0.0-development",
  "description": "Aimee Blue Service Template",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:Aimee-Blue/ab-service-kit.git",
  "author": "@Aimee-Blue",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "src",
    "template"
  ],
  "scripts": {
    "build": "node ./scripts/build.js -s",
    "build:watch": "node ./scripts/build.js -s --watch",
    "declarations": "tsc -p ./tsconfig.declarations.json",
    "prebuild": "node ./scripts/clean.js",
    "clean": "node ./scripts/clean.js",
    "test": "jest --color",
    "integration": "jest --config ./jest.config.integration.js --color -i",
    "integration:inspect": "node --inspect-brk ./node_modules/.bin/jest --config ./jest.config.integration.js --color -i",
    "combine-coverage": "node ./scripts/combineCoverage.js",
    "type-check": "tsc",
    "lint": "tslint -p . -t verbose",
    "check": "concurrently \"node ./scripts/build.js\" \"tsc\" \"tslint -p . -t verbose\"",
    "patch": "ts-deps patch",
    "release": "semantic-release",
    "prepublishOnly": "yarn run build && yarn run declarations",
    "start": "node lib/test",
    "start:watch": "node lib/test --watch",
    "start:inspect": "node --inspect lib/test"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run check"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@aimee-blue/ab-contracts": ">=1.114.0"
  },
  "dependencies": {
    "@aimee-blue/ab-auth": "1.1.0",
    "@google-cloud/pubsub": "0.31.1",
    "@hapi/joi": "15.1.1",
    "clear-module": "4.0.0",
    "cors": "2.8.5",
    "dotenv-expand": "5.1.0",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "node-fetch": "2.6.0",
    "rxjs-spy": "7.5.1",
    "rxjs-stream": "3.0.2",
    "rxjs": "6.5.3",
    "source-map-support": "0.5.13",
    "uuid": "3.3.3",
    "ws": "7.1.2",
    "yargs": "14.0.0"
  },
  "devDependencies": {
    "@aimee-blue/ab-contracts": ">=1.114.0",
    "@types/cors": "2.8.6",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/hapi__joi": "15.0.4",
    "@types/node-fetch": "2.5.0",
    "@types/node": "10",
    "@types/uuid": "3.4.5",
    "@types/ws": "6.0.3",
    "chokidar": "3.0.2",
    "husky": "3.0.5",
    "rxjs-marbles": "5.0.2",
    "ts-deps": "0.4.4",
    "utility-types": "3.7.0"
  },
  "resolutions": {
    "ts-deps/semantic-release/marked": "^0.7.0"
  }
}
