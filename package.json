{
  "name": "@aimee-blue/ab-service-kit",
  "version": "0.0.0-development",
  "private": false,
  "description": "Aimee Blue Service Template",
  "repository": "git@github.com:Aimee-Blue/ab-service-kit.git",
  "license": "MIT",
  "author": "@Aimee-Blue",
  "files": [
    "lib",
    "src",
    "template"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prebuild": "node ./scripts/clean.js",
    "build": "node ./scripts/build.js -s",
    "build:watch": "node ./scripts/build.js -s --watch",
    "check": "concurrently --raw \"node ./scripts/build.js\" \"tsc\" \"tslint -p . -t verbose\"",
    "clean": "node ./scripts/clean.js",
    "combine-coverage": "node ./scripts/combineCoverage.js",
    "declarations": "tsc -p ./tsconfig.declarations.json",
    "integration": "jest --config ./jest.config.integration.js --color -i",
    "integration:inspect": "node --inspect-brk ./node_modules/.bin/jest --config ./jest.config.integration.js --color -i",
    "lint": "tslint -p . -t verbose",
    "patch": "ts-deps patch",
    "prepublishOnly": "yarn run build && yarn run declarations",
    "release": "node ./scripts/release.js",
    "start": "node lib/test",
    "start:inspect": "node --inspect lib/test",
    "start:watch": "node lib/test --watch",
    "test": "jest --color",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run check"
    }
  },
  "lint-staged": {
    "*.{tsx,jsx,ts,js,json,md,yaml,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aimee-blue/ab-auth": "1.4.1",
    "@aimee-blue/ab-shared": "1.118.1",
    "@google-cloud/pubsub": "1.2.0",
    "@hapi/joi": "15.1.1",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "node-fetch": "2.6.0",
    "parent-module": "2.0.0",
    "resolve-from": "5.0.0",
    "rxjs": "6.5.4",
    "rxjs-spy": "7.5.1",
    "rxjs-stream": "3.1.1",
    "source-map-support": "0.5.16",
    "uuid": "3.3.3",
    "ws": "7.2.1",
    "yargs": "15.1.0"
  },
  "devDependencies": {
    "@aimee-blue/ab-contracts": "1.170.1",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/hapi__joi": "15.0.4",
    "@types/node": "10",
    "@types/node-fetch": "2.5.4",
    "@types/uuid": "3.4.6",
    "@types/ws": "6.0.4",
    "chokidar": "3.3.1",
    "husky": "4.0.7",
    "lint-staged": "10.0.0-beta.15",
    "rxjs-marbles": "5.0.4",
    "ts-deps": "0.12.6",
    "utility-types": "3.10.0"
  },
  "peerDependencies": {
    "@aimee-blue/ab-contracts": ">=1.164.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
