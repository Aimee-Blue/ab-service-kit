{
  "name": "@aimee-blue/ab-service-kit",
  "version": "0.0.0-development",
  "description": "Aimee Blue Service Template",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:Aimee-Blue/ab-service-kit.git",
  "author": "@Aimee-Blue",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "src",
    "template"
  ],
  "scripts": {
    "build": "node ./scripts/build.js -s",
    "build:watch": "node ./scripts/build.js -s --watch",
    "declarations": "tsc -p ./tsconfig.declarations.json",
    "prebuild": "node ./scripts/clean.js",
    "clean": "node ./scripts/clean.js",
    "test": "jest --color",
    "integration": "jest --config ./jest.config.integration.js --color -i",
    "combine-coverage": "node ./scripts/combineCoverage.js",
    "type-check": "tsc",
    "lint": "tslint -p . -t verbose",
    "check": "concurrently \"node ./scripts/build.js\" \"tsc\" \"tslint -p . -t verbose\"",
    "patch": "ts-deps patch",
    "release": "semantic-release",
    "prepublishOnly": "yarn run build && yarn run declarations",
    "start": "node lib/test",
    "start:watch": "node lib/test --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run check"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@aimee-blue/ab-shared": ">=1.55.0"
  },
  "dependencies": {
    "@google-cloud/pubsub": "0.28.1",
    "@types/express": "4.16.1",
    "@types/joi": "14.3.3",
    "@types/node-fetch": "2.3.3",
    "@types/ws": "6.0.1",
    "chokidar": "3.0.0",
    "clear-module": "3.2.0",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "dotenv-expand": "5.1.0",
    "express": "4.16.4",
    "fs-extra": "7.0.1",
    "joi": "14.3.1",
    "node-fetch": "2.4.1",
    "rxjs": "6.5.1",
    "rxjs-spy": "7.5.1",
    "rxjs-stream": "3.0.1",
    "source-map-support": "0.5.12",
    "ws": "7.0.0",
    "yargs": "13.2.2"
  },
  "devDependencies": {
    "@aimee-blue/ab-shared": ">=1.55.0",
    "@types/dotenv": "6.1.1",
    "@types/cors": "2.8.5",
    "@types/node": "10.14.4",
    "rxjs-marbles": "5.0.2",
    "ts-deps": "0.3.4",
    "husky": "2.2.0"
  }
}
