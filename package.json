{
  "name": "ab-service-kit",
  "version": "0.0.0-development",
  "description": "Aimee Blue Service Template",
  "main": "index.js",
  "repository": "git@github.com:Aimee-Blue/ab-service-kit.git",
  "author": "@Aimee-Blue",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "node ./scripts/build.js",
    "declarations": "tsc -p ./tsconfig.declarations.json",
    "prebuild": "node ./scripts/clean.js",
    "clean": "node ./scripts/clean.js",
    "test": "jest --color",
    "integration": "jest --config ./jest.config.integration.js --color -i",
    "combine-coverage": "node ./scripts/combineCoverage.js",
    "type-check": "tsc",
    "lint": "tslint -p . -t verbose",
    "check": "concurrently \"node ./scripts/build.js\" \"tsc\" \"tslint -p . -t verbose\"",
    "patch": "ts-deps patch",
    "release": "semantic-release"
  },
  "dependencies": {
    "@aimee-blue/ab-shared": "1.51.0",
    "chokidar": "2.1.5",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "dotenv-expand": "5.1.0",
    "express": "4.16.4",
    "fs-extra": "7.0.1",
    "joi": "14.3.1",
    "mock-socket": "8.0.5",
    "rxjs": "6.4.0",
    "rxjs-spy": "7.5.1",
    "rxjs-stream": "3.0.1",
    "source-map-support": "0.5.11",
    "ws": "6.2.1",
    "yargs": "13.2.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.4",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/joi": "14.3.2",
    "@types/node": "10.14.4",
    "@types/ws": "6.0.1",
    "rxjs-marbles": "5.0.0",
    "rxjs-tslint-rules": "4.19.1",
    "ts-deps": "0.2.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run check"
    }
  }
}
